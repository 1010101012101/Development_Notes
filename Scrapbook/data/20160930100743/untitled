<script type="text/javascript">// <![CDATA[
  
var current = 0 ; 
var xprint = 0;  
var speed = 8;
var speed2 =8000; 
function initArray(n) {  
  this.length = n;  
  for (var i =1; i <= n; i++) {  
    this[i] = ' '  
  }  
}  
typ = new initArray(3);  
typ[0]="406:#339ccc:Technology moves at an astounding pace , and the ones using the tools it produces never seem to lag behind. We here at SainSmart aim to provide all the thinkers , builders , makers and ";
typ[1]="409:#339ccc:doers with every device you need to keep you up to speed . We know unique projects have unique requirements , and we strive to meet them with everything from sensors , motors , testing"; 
typ[2]="384:#339ccc: equipment,3D printing supplies, starter kits and more. We live in a time where the “next big thing” could come from anyone at anytime, and we’re here to support that. Anywhere. Anytime.";  
function typewrite() {  
  var mprint = typ[current];  
  document.getElementById("typewriter").href = '/survey/report.html?rt=' + mprint.substring(0,mprint.indexOf(':'));  
  mprint = mprint.substring(mprint.indexOf(':') + 1);  
  document.getElementById("typewriter").style.color = mprint.substring(0,mprint.indexOf(':'));  
  mprint = mprint.substring(mprint.indexOf(':') + 1);  
  mprint = mprint.replace("&","and");  
  document.getElementById("typewriter").innerHTML = mprint.substring(0, xprint++) + "_";  
    
  if (xprint == mprint.length + 1) {  
    xprint = 0;
    current++;  
    if (current > typ.length - 1) {  
      current = 0 ; 
    }  
    setTimeout("typewrite()", speed2)  
    }  
  else {  
    setTimeout("typewrite()", speed)  
  }  
}  
typewrite()
