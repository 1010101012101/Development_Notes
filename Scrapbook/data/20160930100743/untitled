jQuery(function(){jQuery(window).scroll(function () {
     if(jQuery(window).scrollTop() >=150)//距离顶部多少高度显示按钮
     {jQuery('#goTopBtn').slideDown(200);}else
     {jQuery('#goTopBtn').slideUp(200);}});  
   jQuery('#goTopBtn').click(function(){
    jQuery('body,html').animate({scrollTop:0},500)});//按钮定位
   var win_width= jQuery(window).width();      
   var content_width= jQuery('.wrap').width();     
   var topbtn_width= jQuery('#goTopBtn').width(); //距离主体部分的右侧距离
   var topbtn_posi = ([win_width - content_width ]/2 - topbtn_width - 10);
   jQuery('#goTopBtn').css({'right':0}); })
</script>
