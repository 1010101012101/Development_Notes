var  JQc = jQuery.noConflict();
	JQc(function(){
		if(JQc.cookie("isClose") != 'yes'){
			JQc(".alert_windows").css("display","block");
			JQc("#window-bg").css("display","block");
			JQc(".alert_windows .alert-x").click(function(){
				JQc.cookie("isClose",'yes',{ expires:1/72});	
				JQc(".alert_windows").css("display","none");
				JQc("#window-bg").css("display","none");	
