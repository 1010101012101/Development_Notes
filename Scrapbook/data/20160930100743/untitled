<script type="text/javascript">
          var pPostUrl = 'http://www.sainsmart.com/newsletter/subscriber/ajaxnew/';
          var pFormId = 'newsletter-validate-detail';
          var myPForm = new VarienForm(pFormId,true);
          function postEmars(postUrl2){
             var pFormId = 'newsletter-validate-detail';
             var params = '';
             if(myPForm.validator.validate()){
                 new Ajax.Updater(
                     {success:'formSucess'},
                     postUrl2,
                     {
                       method:'get',
                       asynchronous:true,
                       evalScripts:false,
                       onComplete:function(transport){
                     },
                  parameters:params
                }
              );
          }
         }

          function doAjax(){
            if(myPForm.validator.validate()){
                 new Ajax.Updater(
                         {success:'formSuccess'},
                         pPostUrl,{
                            method:'post',
                            asynchronous:true,
                            evalScripts:false,
                            onComplete:function(transport){
                                if(transport.status == 200){
                                    var result = transport.responseText.evalJSON();
                                    //alert(result.err);alert(result.msg);
                                    $(pFormId).hide();
                                    if(result.err == '1'){
										
                                        $('subscribe-msg').update('Submit failed');
                                        $('subscribe-msg').show();
                                        setTimeout("$('alert_windows').hide();", 3000);
										setTimeout("$('window-bg').hide();", 3000);
                                    }else{
                                        //$('subscribe-msg').update('success');
                                        $('subscribe-msg').show();
                                    }
                                    
                                }else{
                                }
                            },
                            parameters:$(pFormId).serialize(true)
                         }
                    );
            }else{
                //alert('please check your email!');
            }
          }
       
       new Event.observe(pFormId,'submit',function(e){
           e.stop();
           var pfirstName = $('newsf').value;
           var plastName = $('newsl').value;
           var pemailAddr = $('news-email').value;
           var postUrl2  = "https://link.sainsmart.com/u/register.php?CID=398065160&f= 1030&p=2&a=r&SID=&el=&llid=&counted=&c=&optin=[Opt-in]&interest[]=[Interessen]&inp_1="+pfirstName +"&inp_2=" +plastName + "&inp_3=" + pemailAddr + "";
             postEmars(postUrl2);
           doAjax();
         });

      </script>
