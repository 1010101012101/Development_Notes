<script type="text/javascript">
	jQuery(".datepicker").datepicker({ dateFormat: 'yy-mm-dd',  maxDate: 0 });
	function subimt_button(){
		if((new Date(document.getElementById("end_date").value) < new Date(document.getElementById("start_date").value)) || document.getElementById("start_date").value == '') { 
			alert("Please select correct date");
			document.getElementById("end_date").style.border = "1px solid red";
			if(document.getElementById("start_date").value == ''){
				document.getElementById("start_date").style.border = "1px solid red";
			}
			return false;
		}
		else{
			document.getElementById('advance_search_form').submit();
		}
	}
	function clearInput(){
		//alert(document.getElementsByClassName('inp_box').value); 
		document.getElementsByClassName('inp_box')[0].value = '';
		document.getElementsByClassName('inp_box')[1].value = '';
		document.getElementsByClassName('inp_box')[2].value = '';
		document.getElementsByClassName('inp_box')[4].value = '';
	}
	
</script>
