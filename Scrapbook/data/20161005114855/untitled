	jQuery(document).ready(function(){
		/* jQuery for floating top menu */
		if(jQuery(window).width() > 768){
		var height = jQuery("header.header-main").height(); 
		if(jQuery(window).scrollTop() > height){
			//jQuery("div.middle-header").show();
			jQuery("div.middle-header").addClass('navbar-fixed-top');
		}
		jQuery( window ).scroll(function() {
			if(jQuery(window).scrollTop() > height){
				//jQuery("div.middle-header").show();
				jQuery("div.middle-header").addClass('navbar-fixed-top');
			}
			else{
				//jQuery("div.middle-header").hide();
				jQuery("div.middle-header").removeClass('navbar-fixed-top');
				
			}
		});
		}
		
		/* jQuery for floating top menu Ends */
		
		var height = jQuery(".article-page-links h2").height(); 
		if(jQuery(window).scrollTop() > height){
			//jQuery("div.middle-header").show();
			
		}
		jQuery( window ).scroll(function() {
			if(jQuery(window).scrollTop() > height){
				//jQuery("div.middle-header").show();
				jQuery("#sharing_single_post").addClass('sharing-fixed');
				if(jQuery(window).width() <= 768){
					jQuery("#sharing_single_post").css('top','0px');
				}
			}
			else{
				//jQuery("div.middle-header").hide();
				jQuery("#sharing_single_post").removeClass('sharing-fixed');
				
			}
		});
	});
		</script>
		
		
